<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="1.7976931348623157E308"
      maxWidth="1.7976931348623157E308"
      spacing="5.0" xmlns="http://javafx.com/javafx/8.0.40"
      fx:controller="cz.muni.sci.astro.fhm.gui.ProgressInfoController">
    <children>
        <ProgressIndicator fx:id="progressIndicator" progress="0.0"/>
        <Label fx:id="labelProcessInfo" text="ProcessInfo"/>
        <HBox alignment="CENTER" spacing="5.0">
            <children>
                <Label minWidth="75.0" text="Processing:">
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="labelActualProcessing" text="ActualProcessing"/>
            </children>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
